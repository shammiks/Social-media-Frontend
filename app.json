{
  "expo": {
  "name": "AspireHub",
  "slug": "aspirehub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aspirehub.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
  "permissions": [
    "READ_EXTERNAL_STORAGE",
    "WRITE_EXTERNAL_STORAGE"
  ],
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/aspirehub.png",
    "backgroundColor": "#ffffff"
  }
},

    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
